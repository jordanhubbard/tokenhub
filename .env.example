# TokenHub Environment Configuration
# Copy this file to .env and configure your settings

# Admin password for vault access (REQUIRED)
TOKENHUB_ADMIN_PASSWORD=changeme

# Server configuration
TOKENHUB_HOST=0.0.0.0
TOKENHUB_PORT=8080

# Database path
TOKENHUB_DB_PATH=tokenhub.db

# Debug mode
TOKENHUB_DEBUG=false

